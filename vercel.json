{
    "version": 2,
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "src/index.ts" }
    ],
    "builds": [
      { "src": "src/index.ts", "use": "@vercel/node" },
      { "src": "static/*", "use": "@vercel/static" }
    ],
    "env": {
      "YARN_NODE_LINKER": "node-modules"
    },
    "build": {
      "env": {
        "YARN_NODE_LINKER": "node-modules"
      }
    }
}